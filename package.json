    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:codegen": "playwright codegen http://localhost:3000"
  },
  "dependencies": {
    "@types/three": "^0.179.0",
    "lucide-react": "^0.525.0",
